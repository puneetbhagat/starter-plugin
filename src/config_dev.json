{
    "id": "starter-plugin",
    "hasLayers": false,
    "manifestVersion": "2",
    "allowedOrigins": [
        "*"
    ],
    "subscribedEvents": [
        "onLoad"
    ],
    "extensions": {
        "universal/tx-form": [
            {
                "type": "bill",
                "controller": "qbo-ui-libs-plugins/addon/iFrameViewController",
                "iFrameAttachPoint": "alternativePayment",
                "xdmParameters": {
                    "sourcePath": "/salestax.html"
                }
            }
        ]
    }
}
